{
  "convention": {
    "commitTypes": [
      "feat",
      "fix",
      "perf",
      "refactor",
      "style",
      "test",
      "build",
      "ops",
      "docs",
      "chore",
      "merge",
      "revert"
    ],
    "commitScopes": [],
    "releaseTagGlobPattern": "[0-9]*.[0-9]*.[0-9]*",
    "issueRegexPattern": "(^|\\s)#\\d+(\\s|$)"
  },
  "changelog": {
    "commitTypes": [
      "feat",
      "fix",
      "perf",
      "merge"
    ],
    "includeInvalidCommits": true,
    "commitIgnoreRegexPattern": "^WIP ",
    "headlines": {
      "feat": "Features",
      "fix": "Bug Fixes",
      "perf": "Performance Improvements",
      "merge": "Merged Branches",
      "breakingChange": "BREAKING CHANGES"
    },
    "commitUrl": "https://github.com/lightness/release-it-slack-notification-plugin/commit/%commit%",
    "commitRangeUrl": "https://github.com/lightness/release-it-slack-notification-plugin/compare/%from%...%to%?diff=split",
    "issueUrl": "https://github.com/lightness/release-it-slack-notification-plugin/issues/%issue%"
  }
}
